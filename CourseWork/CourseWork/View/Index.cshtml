
  @Entities List<Teacher>
@{
    ViewData["Title"] = "Home";
}


   
      <h2>Persons</h2>
    <table class="table table-bordered" >
        <thead>
            <tr>
                <th>Id</th>
                <th>First name</th>
                <th>Last name</th>
                <th>age</th>
            </tr>

        </thead>
    </table>

  <tbody>

        @foreach(var item in Model)
        {
        if (@item.Id == 1)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Age</td>
            </tr>
        }
    }
    </tbody>

